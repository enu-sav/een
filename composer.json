{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "canonical": false,
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "drupal/forum_access": {
            "canonical": false,
            "type": "vcs",
            "url": "https://git.drupalcode.org/issue/forum_access-3410468.git"
        }
    },
    "require": {
        "php": ">=8.0",
        "composer/installers": "^1.12",
        "cweagans/composer-patches": "^1.7",
        "drupal/acl": "^1.1",
        "drupal/admin_local_tasks": "^1.1",
        "drupal/admin_toolbar": "^3.5",
        "drupal/advpoll": "^1.2",
        "drupal/antibot": "^2.0",
        "drupal/config_ignore": "^3.3",
        "drupal/content_moderation_notifications": "^3.7",
        "drupal/content_moderation_owner_permissions": "^1.0@alpha",
        "drupal/core": "10.4.2",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-recommended": "^10",
        "drupal/diff": "^1.8",
        "drupal/environment_indicator": "^4.0",
        "drupal/form_mode_control": "^2.4",
        "drupal/forum_access": "dev-3410468-24",
        "drupal/forum_notifications_subscription": "^2.1",
        "drupal/gin": "4.0.2",
        "drupal/gin_toolbar": "^2.0",
        "drupal/hook_post_action": "^1.2",
        "drupal/layout_options": "^1.7",
        "drupal/layout_paragraphs": "^2.1",
        "drupal/mailsystem": "^4.5",
        "drupal/media_library_form_element": "^2.1",
        "drupal/menu_per_role": "^1.8",
        "drupal/metatag": "^2.1",
        "drupal/mimemail": "^1.0.0-alpha6",
        "drupal/paragraphs": "^1.18",
        "drupal/paragraphs_ee": "^10.0",
        "drupal/paragraphs_features": "^2.1",
        "drupal/pathauto": "^1.13",
        "drupal/phpmailer_smtp": "^2.2",
        "drupal/poll": "^1.6",
        "drupal/rabbit_hole": "^2.0@beta",
        "drupal/rdf": "^2.1",
        "drupal/recreate_block_content": "^3.0",
        "drupal/reroute_email": "^2.2",
        "drupal/simple_sitemap": "^4.2",
        "drupal/stage_file_proxy": "^3.1",
        "drupal/statistics": "^1.0",
        "drupal/svg_image": "^3.2",
        "drupal/token": "^1.15",
        "drupal/twig_tweak": "^3.4",
        "drupal/upgrade_status": "^4.3",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webp": "^1.0@RC",
        "drupal/workbench": "^1.6",
        "drupal/workbench_access": "^2.0",
        "drupal/workbench_moderation": "^1.8",
        "drush/drush": "^12.5",
        "mglaman/composer-drupal-lenient": "^1.0",
        "vlucas/phpdotenv": "^5.6",
        "webflo/drupal-finder": "^1.3"
    },
    "require-dev": {
        "drupal/core-dev": "^10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "mglaman/composer-drupal-lenient": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/.htaccess": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/advpoll", "drupal/acl", "drupal/forum_access"]
        },
        "patches": {
            "drupal/core": {
                "2010132 - Canonical taxonomy term link for forum vocabulary is broken": "https://www.drupal.org/files/issues/2022-12-30/2010132-154.patch"
            },
            "drupal/poll": {
                "3007808 - Fine grain control over results display": "https://www.drupal.org/files/issues/2021-05-06/display_results_option--3007808-8.patch"
            },
            "drupal/advpoll": {
                "3296549 - Automated Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2023-04-20/advpoll.1.x-dev-3296549%233.patch"
            },
            "drupal/acl": {
                "D10 compatibility": "https://www.drupal.org/files/issues/2023-12-21/acl_beta_1_drupal_10.patch"
            },
            "drupal/forum_notifications_subscription": {
                "3501543 - ActionLinkController::getSubscriptionLink() should attach the core/drupal.ajax library": "https://www.drupal.org/files/issues/2025-01-22/forum_notifications_subscription-attach_ajax_lib-3501543-2.patch",
                "3491272 - Empty legend tag": "https://git.drupalcode.org/project/forum_notifications_subscription/-/merge_requests/9.diff"
            }
        }
    }
}
